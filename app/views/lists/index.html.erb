<div class="container">
  <div class="app-bar-top">
    <%= link_to products_path do %>
      <span class="material-icons black-text">chevron_left</span>
    <% end %>
  </div>
</div>

<div class="container">
  <h2>Previous shopping</h2>
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do %>
        <div class="list-item-container">
          <h6><%= list.date %></h6>
        </div>
      <% end %>
    <% end %>
</div>
