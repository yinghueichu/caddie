<div class="container">
  <div class="app-bar-top">
    <%= link_to products_path do %>
      <span class="material-icons black-text">chevron_left</span>
    <% end %>
  </div>
</div>

<div class="container">
  <h2>Previous shopping</h2>
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do %>
        <div class="list-history-container">
          <h6 class="list-date"><%= list.date.strftime("%d/%m/%Y") %></h6>
          <p class="list-user"><%= list.user.name %></p>
        </div>
      <% end %>
    <% end %>
</div>
